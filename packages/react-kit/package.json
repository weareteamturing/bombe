    "@types/styled-system": "^5.1.17",
    "@types/styled-system__css": "^5.0.17",
    "csstype": "^3.1.2",
    "react": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "styled-components": "^6.0.7"
  },
  "peerDependencies": {
    "@styled-system/css": "^5.1.5",
    "@types/styled-system": "^5.1.17",
